{
  "name": "users",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "rimraf dist && tsc -p tsconfig.build.json",
    "start": "nest start",
    "generate:key": "openssl rand -base64 756 > ./db/replica-set.key && chmod 400 ./db/replica-set.key",
    "db:up": "docker-compose up -d users-db",
    "rs:init": "npm run build && node -r dotenv/config dist/scripts/replica-set.init.js dotenv_config_path=./.env",
    "rs:conf": "npm run build && node -r dotenv/config dist/scripts/replica-set.conf.js dotenv_config_path=./.env",
    "db:stop": "docker-compose stop users-db",
    "db:rm": "docker-compose stop users-db && docker-compose rm -fs users-db",
    "start:dev": "npm run build && tsc-watch -p tsconfig.build.json --onSuccess \"node -r dotenv/config dist/main.js dotenv_config_path=./.env\"",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@nestjs/common": "9.1.4",
    "@nestjs/config": "2.2.0",
    "@nestjs/core": "9.1.4",
    "@nestjs/jwt": "9.0.0",
    "@nestjs/mapped-types": "1.2.0",
    "@nestjs/microservices": "9.1.4",
    "@nestjs/mongoose": "9.2.0",
    "@nestjs/platform-fastify": "9.1.4",
    "@nestjs/schedule": "2.1.0",
    "bcrypt": "5.1.0",
    "class-transformer": "0.5.1",
    "class-validator": "0.13.2",
    "mongoose": "6.6.5",
    "passport-jwt": "4.0.0",
    "reflect-metadata": "0.1.13",
    "rimraf": "3.0.2",
    "rxjs": "7.5.7",
    "sanitize-html": "2.7.3"
  },
  "devDependencies": {
    "@nestjs/cli": "9.1.4",
    "@nestjs/schematics": "9.0.3",
    "@nestjs/swagger": "6.1.2",
    "@nestjs/testing": "9.1.4",
    "@types/jest": "29.1.2",
    "@types/node": "18.8.3",
    "@types/passport-jwt": "3.0.7",
    "@types/supertest": "2.0.12",
    "@typescript-eslint/eslint-plugin": "5.39.0",
    "@typescript-eslint/parser": "5.39.0",
    "dotenv": "16.0.3",
    "eslint": "8.25.0",
    "eslint-plugin-simple-import-sort": "8.0.0",
    "exec-sh": "0.4.0",
    "jest": "29.1.2",
    "source-map-support": "0.5.21",
    "supertest": "6.3.0",
    "ts-jest": "29.0.3",
    "ts-loader": "9.4.1",
    "ts-node": "10.9.1",
    "tsc-watch": "5.0.3",
    "tsconfig-paths": "4.1.0",
    "typescript": "4.8.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
