{
  "name": "file-processor",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "license": "UNLICENSED",
  "scripts": {
    "build": "rimraf dist && tsc -p tsconfig.build.json",
    "start": "nest start",
    "start:dev": "npm run build && tsc-watch -p tsconfig.build.json --onSuccess \"node -r dotenv/config dist/main.js dotenv_config_path=./.env\"",
    "start:test": "tsc-watch -p tsconfig.build.json --onSuccess \"node -r dotenv/config dist/main.js dotenv_config_path=./.env.test\"",
    "start:prod": "node dist/main.js",
    "lint": "eslint \"src/**/*.ts\" --fix"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "3.194.0",
    "@aws-sdk/s3-request-presigner": "3.194.0",
    "@aws-sdk/util-endpoints": "3.194.0",
    "@nestjs/common": "9.1.4",
    "@nestjs/config": "2.2.0",
    "@nestjs/core": "9.1.4",
    "@nestjs/microservices": "9.1.4",
    "@nestjs/platform-express": "9.1.4",
    "@nestjs/schedule": "2.1.0",
    "class-transformer": "0.5.1",
    "class-validator": "0.13.2",
    "dayjs": "1.11.5",
    "lodash.clonedeep": "4.5.0",
    "reflect-metadata": "0.1.13",
    "rxjs": "7.5.7",
    "uuid": "9.0.0"
  },
  "devDependencies": {
    "@nestjs/cli": "9.1.4",
    "@nestjs/schematics": "9.0.3",
    "@nestjs/testing": "9.1.4",
    "@types/cron": "2.0.0",
    "@types/express": "4.17.14",
    "@types/multer": "1.4.7",
    "@types/node": "18.11.0",
    "@typescript-eslint/eslint-plugin": "5.40.0",
    "@typescript-eslint/parser": "5.40.0",
    "dotenv": "16.0.3",
    "eslint": "8.25.0",
    "eslint-plugin-simple-import-sort": "8.0.0",
    "rimraf": "3.0.2",
    "ts-node": "10.9.1",
    "tsc-watch": "5.0.3",
    "tsconfig-paths": "4.1.0",
    "typescript": "4.8.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
